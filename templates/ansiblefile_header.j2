- hosts: {{ vm['hostname'] }}
  gather_facts: true
{#-vars_files:
  {% for service in vm['service'] %}
    - {{ conductor_path }}/vars/{{ service }}.yml
  {% endfor %}
#}
{%- if vm['services']|length %}
  vars:
{%- for service in vm['services'] %}
  {%- if service['parameter'] is not none and service['parameter']['Error'] is not defined %}
    {%- for key,value in service['parameter'].items() %}
    {{ key }}: {{ value }}
    {%- endfor %}
  {%- endif %}
{%- endfor %}
{%- else %}
  vars: null
{%- endif %}
  tasks:

